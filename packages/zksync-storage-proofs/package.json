{
    "name": "@getclave/zksync-storage-proofs",
    "main": "build/cjs/index.js",
    "module": "build/esm/index.js",
    "types": "build/esm/index.d.ts",
    "devDependencies": {
        "@types/node": "^20.11.16",
        "bun-types": "latest",
        "nodemon": "^3.0.3",
